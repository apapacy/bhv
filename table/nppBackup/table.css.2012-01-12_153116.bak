table {
  background-color:#B0C4DE;
  color:#000099
}

td {
  border-width:1pt;
  border-style:inset;
  vertical-align:top
}

th {
  border-width:1pt;
  border-style:inset
}

table.noborder td {
  border-width:0
}

table.noborder th{
  border-width:0
}

div.bhv_table {
	background-color:#dddddd;
	padding: 4px 4px 4px 4px;
	margin: 8px 8px 8px 8px;
}

div.bhv_table_head{
	padding: 4px 4px 4px 4px;
	margin: 8px 8px 8px 8px;
	height: 50px;
}

div.bhv_table_column_head{
	float:left;
	width:150px;
	padding: 2px;
	background-color: #bbcccc;
	overflow:hidden;
}

div.bhv_table_row, div.bhv_table_current_row{
	padding: 4px 4px 4px 4px;
	margin: 2px 2px 2px 2px;
	height: 24px;
	overflow:hidden;	
}

div.bhv_table_ceil{
	float: left;
	width: 150px;
	padding: 2px;
	background-color: #bbcccc;
	overflow:hidden;
}

div.bhv_table_current_row  div.bhv_table_ceil{
	background-color: #dddddd;
}

span.pressedSilverButton {
  background-color: #c0c0c0;
  padding: 0px 4px 0px 4px;
  margin: 0px;
  border-color: #dddddd;
  border-width: 2px;
  border-style: inset
}

span.silverButton {
  background-color: #c0c0c0;
  padding: 0px 4px 0px 4px;
  margin: 0px;
  border-color: #dddddd;
  border-width: 2px;
  border-style: outset
}

span.page {
  background-color: silver;
  line-height: 30px;
  padding: 2px 4px 2px 4px;
  margin: 2px 
}

span.selectedPage {
  background-color: yellow;
  line-height: 30px;
  padding: 2px 4px 2px 4px;
  margin: 2px 
}
